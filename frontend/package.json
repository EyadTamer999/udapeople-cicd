{
  "name": "typescript-react-redux-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A frontend boilerplate with React, Redux and Typescript",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "pretest:cov": "npm run lint",
    "test:cov": "jest --coverage",
    "start": "webpack-dev-server --mode development --hot --progress --color --port 3000 --open",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build:prod": "webpack -p --progress --colors",
    "build": "webpack --mode development --progress --colors",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "2.2.9",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "4.7.2",
    "@types/jest": "^24.9.1",
    "@types/node": "12.6.2",
    "@types/react": "16.8.23",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.1.1",
    "@types/react-router": "5.0.3",
    "@types/react-router-redux": "^5.0.21",
    "@types/react-table": "^6.8.9",
    "@types/react-toastify": "^4.1.0",
    "@types/redux-actions": "2.6.1",
    "@types/webpack": "4.4.34",
    "@types/webpack-env": "1.13.9",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.5",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "file-loader": "^4.3.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.2",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss": "^7.0.39",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.1",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.13.0",
    "react-test-renderer": "^16.14.0",
    "redux-devtools-extension": "^2.13.9",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.10",
    "url-loader": "^2.3.0",
    "webpack": "^4.46.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.19",
    "@types/react-datepicker": "^2.11.1",
    "@types/react-gravatar": "^2.6.10",
    "@types/react-tagsinput": "^3.19.9",
    "@types/redux-form": "^8.3.5",
    "axios": "^0.19.2",
    "axui-contextmenu": "^0.4.0",
    "classnames": "^2.3.1",
    "connected-react-router": "^4.5.0",
    "dotenv": "^8.6.0",
    "dotenv-webpack": "^1.8.0",
    "jest": "^24.9.0",
    "moment": "^2.29.4",
    "oauth-sign": "^0.9.0",
    "raf": "^3.4.1",
    "react": "^16.14.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-country-region-selector": "^1.4.7",
    "react-datepicker": "^2.16.0",
    "react-dom": "^16.14.0",
    "react-foundation": "^0.9.7",
    "react-gravatar": "^2.6.3",
    "react-loading": "^2.0.3",
    "react-redux": "^5.1.2",
    "react-router": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-table": "^6.11.5",
    "react-tagsinput": "^3.19.0",
    "react-toast-notifications": "^2.5.1",
    "react-toastify": "^5.5.0",
    "redux": "^4.2.0",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.4.1",
    "sweetalert2": "^8.19.0"
  },
  "jest": {
    "rootDir": "src",
    "coverageDirectory": "../coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/app/**/*.(ts|tsx|js)",
      "!<rootDir>/app/index.tsx",
      "!<rootDir>/app/containers/App/*",
      "!<rootDir>/app/(reducers|components|store|services)/index.ts",
      "!<rootDir>/app/(middleware|actions|containers|models|utils)/*",
      "!<rootDir>/app/components/(Error|Callback)/*",
      "!<rootDir>/app/services/(httpService).ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": ".*(spec|test)\\.(ts|tsx|js)$",
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.ts",
    "snapshotSerializers": [
      "enzyme-to-json"
    ],
    "moduleNameMapper": {
      "^app/(.*)$": "<rootDir>/app/$1",
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "homepage": "http://mywebsite.com/relativepath"
}
